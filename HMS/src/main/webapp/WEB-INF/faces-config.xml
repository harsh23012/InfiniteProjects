<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://java.sun.com/xml/ns/javaee /WEB-INF/schema/web-facesconfig_2_0.xsd"
              version="2.0" metadata-complete="false">
    <!-- This file must be present with our jar, even if it is empty.  Otherwise, our Java annotations won't get scanned! -->
<managed-bean>
	<managed-bean-name>doctorDao</managed-bean-name>
    <managed-bean-class>com.java.hms.dao.DoctorDaoImpl</managed-bean-class>
    <managed-bean-scope>session</managed-bean-scope>
</managed-bean>
<managed-bean>
	<managed-bean-name>patientDao</managed-bean-name>
    <managed-bean-class>com.java.hms.dao.PatientDaoImpl</managed-bean-class>
    <managed-bean-scope>session</managed-bean-scope>
</managed-bean>
<managed-bean>
	<managed-bean-name>medicalHistoryDao</managed-bean-name>
    <managed-bean-class>com.java.hms.dao.MedicalHistoryDaoImpl</managed-bean-class>
    <managed-bean-scope>session</managed-bean-scope>
</managed-bean>
<managed-bean>
	<managed-bean-name>doctor</managed-bean-name>
    <managed-bean-class>com.java.hms.model.Doctor</managed-bean-class>
    <managed-bean-scope>session</managed-bean-scope>
</managed-bean>
<managed-bean>
	<managed-bean-name>patient</managed-bean-name>
    <managed-bean-class>com.java.hms.model.Patient</managed-bean-class>
    <managed-bean-scope>session</managed-bean-scope>
</managed-bean>
<managed-bean>
	<managed-bean-name>medicalHistory</managed-bean-name>
    <managed-bean-class>com.java.hms.model.MedicalHistory</managed-bean-class>
    <managed-bean-scope>session</managed-bean-scope>
</managed-bean>
<managed-bean>
	<managed-bean-name>hospitalController</managed-bean-name>
    <managed-bean-class>com.java.hms.controller.HospitalController</managed-bean-class>
    <managed-bean-scope>session</managed-bean-scope>
    <managed-property>
            <property-name>doctorDao</property-name>
            <property-class>com.java.hms.dao.DoctorDaoImpl</property-class>
            <value>#{doctorDao}</value>
    </managed-property>
    <managed-property>
            <property-name>doctor</property-name>
            <property-class>com.java.hms.model.Doctor</property-class>
            <value>#{doctor}</value>
    </managed-property>
    <managed-property>
            <property-name>patientDao</property-name>
            <property-class>com.java.hms.dao.PatientDaoImpl</property-class>
            <value>#{patientDao}</value>
    </managed-property>
    <managed-property>
            <property-name>patient</property-name>
            <property-class>com.java.hms.model.Patient</property-class>
            <value>#{patient}</value>
    </managed-property>
    <managed-property>
            <property-name>medicalHistoryDao</property-name>
            <property-class>com.java.hms.dao.MedicalHistoryDaoImpl</property-class>
            <value>#{medicalHistoryDao}</value>
    </managed-property>
    <managed-property>
            <property-name>medicalHistory</property-name>
            <property-class>com.java.hms.model.MedicalHistory</property-class>
            <value>#{medicalHistory}</value>
    </managed-property>
</managed-bean>
</faces-config>