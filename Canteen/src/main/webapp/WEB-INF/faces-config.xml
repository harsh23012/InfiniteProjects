<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://java.sun.com/xml/ns/javaee /WEB-INF/schema/web-facesconfig_2_0.xsd"
              version="2.0" metadata-complete="false">
    <!-- This file must be present with our jar, even if it is empty.  Otherwise, our Java annotations won't get scanned! -->
<managed-bean>
	<managed-bean-name>restaurantDao</managed-bean-name>
    <managed-bean-class>com.java.cntn.dao.RestaurantDaoImpl</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
</managed-bean>
<managed-bean>
	<managed-bean-name>restaurant</managed-bean-name>
    <managed-bean-class>com.java.cntn.model.Restaurant</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
</managed-bean>
<managed-bean>
	<managed-bean-name>restaurantController</managed-bean-name>
    <managed-bean-class>com.java.cntn.controller.RestaurantController</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
    <managed-property>
            <property-name>restaurantDao</property-name>
            <property-class>com.java.cntn.dao.RestaurantDaoImpl</property-class>
            <value>#{restaurantDao}</value>
    </managed-property>
    <managed-property>
            <property-name>restaurant</property-name>
            <property-class>com.java.cntn.model.Restaurant</property-class>
            <value>#{restaurant}</value>
    </managed-property>
</managed-bean>
</faces-config>